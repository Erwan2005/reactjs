() {\n  const user = useSelector((state) => state.user.currentUser);\n  const client = new QueryClient();\n  return (\n   <BrowserRouter>\n      <QueryClientProvider client={client}>\n        <Switch>\n          <Route path=\"/\">\n            { user ? <Home /> : <User />}\n          </Route>\n        </Switch>\n        <ToastContainer />\n      </QueryClientProvider>\n    </BrowserRouter>\n  );\n}","/home/erwan/Documents/reactjs/src/context/store.js",[],[],"/home/erwan/Documents/reactjs/src/context/cartRedux.js",[],[],"/home/erwan/Documents/reactjs/src/context/userRedux.js",[],[],"/home/erwan/Documents/reactjs/src/components/Home/index.jsx",["226","227","228","229","230","231","232","233","234","235","236"],[],"/home/erwan/Documents/reactjs/src/components/User/index.jsx",[],[],"/home/erwan/Documents/reactjs/src/requestMethods.js",[],[],"/home/erwan/Documents/reactjs/src/components/Profile/index.jsx",["237","238","239","240"],[],"/home/erwan/Documents/reactjs/src/components/Messenger/index.jsx",["241","242","243","244","245","246","247"],[],"/home/erwan/Documents/reactjs/src/components/Modal/index.jsx",["248","249","250","251","252"],[],"import React, { Component } from 'react'\nimport { connect } from \"react-redux\";\nimport { publicRequest, userRequest } from '../../requestMethods';\nimport { toast } from 'react-toastify';\nimport './style.css'\nexport class index extends Component {\n    constructor(props){\n\t\tsuper(props);\n        this.state ={\n            oldpass: '',\n            newpass: '',\n            repeatpass: '',\n            error: false,\n            textError: '',\n            disable: true,\n        }\n\t\t\n\t}\n    close = () =>{\n\t\tthis.props.handleClose()\n\t}\n    changepwd = async()=>{\n        try{\n            let data = await userRequest.put('change-password/',{old_password: this.state.oldpass,new_password: this.state.newpass})\n            .then(({data}) => data)\n            toast.success('Password changed sucessffuly')\n            this.props.handleClose()\n        }catch (error){\n            this.setState({error: true, textError: 'Old password is wrong'})\n        }\n    }\n    oldPass = (e) =>{\n        this.setState({oldpass: e.target.value, error: false})\n    }\n\n    newPass = (e) =>{\n        const strongRegex = new RegExp(\"^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[!@#\\$%\\^&\\*])(?=.{8,})\");\n        if(strongRegex.test(e.target.value)) {\n            this.setState({error: false})\n        }else{\n            this.setState({error: true, textError: 'Paswword must have uppercase, lowercase, special character, number and minimum 8 characters'})\n        }\n        this.setState({newpass: e.target.value})\n        \n    }\n    repeatPass = (e) =>{\n        if(e.target.value !== this.state.newpass){\n            this.setState({error: true, textError: \"Password don't match\", repeatpass: e.target.value})\n        }else{\n            this.setState({error: false, repeatpass: e.target.value, disable: false})\n        }\n    }\n    render() {\n        return (\n            <div className='modal'>\n                <div className='container'>\n                    <span className='icon red' onClick={this.close}><ion-icon name=\"close-outline\"/></span>\n                    <span className='header'>Password change</span>\n                    <div className='contents'>\n                        <input type='password' placeholder='Old password' \n                        value = {this.state.oldpass}\n                        onChange = {this.oldPass}/>\n                        <input type='password' placeholder='New password' \n                        value = {this.state.newpass}\n                        onChange = {this.newPass}/>\n                        <input type='password' placeholder='Repeat password' \n                        value = {this.state.repeatpass}\n                        onChange = {this.repeatPass}/>\n                        {this.state.error && <small>{this.state.textError}</small>}\n                        <button onClick={this.changepwd} disabled={this.state.disable}>Change</button>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n}\n\nconst mapStateToProps = (state) => ({\n\tuser: state.user.currentUser,\n});\nexport default connect(mapStateToProps, null)(index)","/home/erwan/Documents/reactjs/src/components/Settings/index.jsx",["253","254","255","256","257","258","259","260","261"],[],"/home/erwan/Documents/reactjs/src/components/Login/index.jsx",["262"],[],"/home/erwan/Documents/reactjs/src/components/Create/index.jsx",["263","264"],[],"import React from 'react'\nimport { withRouter } from \"react-router\";\nimport { Link, Typography, CircularProgress } from '@material-ui/core';\nimport { publicRequest, userRequest } from '../../requestMethods';\nimport FormInput from \"../formInput/FormInput\";\nimport './style.css'\nexport class index extends React.Component {\n  constructor(props) {\n    super(props);\n    this.defaultDark = window.matchMedia('(prefers-color-scheme: dark)').matches;\n    this.state = {\n      values: ({\n        username: \"\",\n        email: \"\",\n        birthday: \"\",\n        password: \"\",\n        confirmPassword: \"\",\n      }),\n      theme: '',\n\n    };\n\n    this.inputs = [\n      {\n        id: 1,\n        name: \"username\",\n        type: \"text\",\n        placeholder: \"Username\",\n        errorMessage:\n          \"Username should be 3-16 characters\",\n        label: \"Username\",\n        pattern: \"^[A-Za-z0-9]{3,16}$\",\n        required: true,\n      },\n      {\n        id: 2,\n        name: \"email\",\n        type: \"email\",\n        placeholder: \"Email\",\n        errorMessage: \"It should be a valid email address!\",\n        label: \"Email\",\n        required: true,\n      },\n      {\n        id: 3,\n        name: \"birthday\",\n        type: \"date\",\n        placeholder: \"Birthday\",\n        label: \"Birthday\",\n      },\n      